{
    "name": "pandawa/workflow",
    "description": "Symfony workflow component for Pandawa",
    "keywords": ["laravel", "pandawa", "workflow"],
    "license": "MIT",
    "type": "library",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Iqbal Maulana",
            "email": "iq.bluejack@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "symfony/workflow": "^5.0",
        "pandawa/pandawa": "^3.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Pandawa\\Workflow\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        },
        "laravel": {
            "providers": [
                "Pandawa\\Workflow\\PandawaWorkflowModule"
            ]
        }
    }
}
