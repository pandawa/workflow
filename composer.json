{
    "name": "pandawa/workflow",
    "description": "Symfony workflow component for Pandawa",
    "keywords": ["laravel", "pandawa", "workflow"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Iqbal Maulana",
            "email": "iq.bluejack@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/workflow": "^6.0",
        "symfony/event-dispatcher": "^6.0",
        "pandawa/eloquent": "^5.0",
        "pandawa/event": "^5.0",
        "pandawa/foundation-bundle": "^5.0",
        "pandawa/dependency-injection-bundle": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Pandawa\\Workflow\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0-dev"
        },
        "laravel": {
            "providers": [
                "Pandawa\\Workflow\\WorkflowBundle"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
